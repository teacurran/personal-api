<FindBugsFilter>
	<Match>
		<!-- Hibernate Generated metamodel classes end in "_"
		 ie: com.whatever.data.model.Book_
		 -->
		<Class name="~.*_"/>
	</Match>

	<Match>
		<!-- Don't complain about: "consider declaring a serialVersionUID" -->
		<Bug pattern="SE_NO_SERIALVERSIONID" />
	</Match>

</FindBugsFilter>